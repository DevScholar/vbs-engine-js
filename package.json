{
  "name": "@devscholar/vbs-engine-js",
  "version": "0.0.3",
  "type": "module",
  "main": "dist/vbs-engine.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vbs-engine.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "node --experimental-transform-types --experimental-vm-modules node_modules/vitest/vitest.mjs",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^7.3.1",
    "vitest": "^3.0.0"
  }
}
